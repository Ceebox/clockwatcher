{
  "productName": "spotifyrust",
  "version": "0.1.0",
  "identifier": "com.spotifyrust.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "spotifyrust",
        "width": 350,
        "height": 150,
        "decorations": false,
        "resizable": false,
        "closable": true,
        "minimizable": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "window": [
      {
        "allow": {
          "all": true
        }
      }
    ]
  }
}
